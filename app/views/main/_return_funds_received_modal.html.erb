<!-- Modal -->
<div class="modal fade" id="funds_received" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="text-align:left">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Cerrar</span></button>
        <h4 class="modal-title" id="myModalLabel">Marcar como fondos recibidos</h4>
      </div>
      <div class="modal-body">
		<p>Marcaras como fondos recibidos los items que seleccionastes</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
		<%= f.submit 'Fondos Recibidos', :class => 'btn btn-primary' %>
      </div>
    </div>
  </div>
</div>